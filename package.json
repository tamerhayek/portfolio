		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.20.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.14",
		"daisyui": "^5.0.6",
		"dotenv": "^16.4.7",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^2.46.1",
